
.tabbar-wrap {
    display: flex;
    justify-content: space-around;
    align-items: center;
    width: 100%;
    height: 89rpx;
    font-size: 20rpx;
    color: #666;
    background: #fafafa;
    position: fixed;
    bottom: 0;
    left: 0;
    border-top: 1rpx solid #ebebeb;
}

.tabbar-item {
    flex: auto;
    text-align:center;
    position: relative;
}

.tabbar-item.active {
    color: #ff7919;
}

.tabbar-item .icon{
  font-size: 44rpx;
}
.tabbar-text {
  font-size: 24rpx;
  line-height: 24rpx;
}