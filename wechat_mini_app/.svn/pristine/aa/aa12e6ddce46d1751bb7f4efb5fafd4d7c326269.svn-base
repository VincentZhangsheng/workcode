.wxSearch{
  position: absolute;
  top: 43rpx;
  left: 0;
  width: 100%;
  height: 100%;
  border-top: 1px #eee solid;
  background-color: rgba(0, 0, 0, 0.2);
  z-index: 99;
}
.wxSearchInner{
  background-color: #fff;
}
.wxSearch-section{
  background-color: #fff;
  padding: 5px 10px;
  border: 1px #eee solid;
}
.wxSearch-pancel{
  display: flex;
  flex-direction: row;
}
.wxSearch-input{
  flex: 8; 
  background-color: #eee;
  padding: 5px 10px;
}
.wxSearch-button{
  flex: 1;
  border: none !important;
  color: cornflowerblue !important;
}

.wxSearchTitle{
  font-size: 24rpx;
  color: #666;
  margin:0 20rpx;
  text-align:left;
}


.wxSearchMindKeyItem{
  padding: 10rpx 5rpx;
  border-bottom: 1rpx solid #eee;
  display: flex;
  font-size: 28rpx;
  color: cornflowerblue;
}

.wxSearchKeyList{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  margin: 0 20rpx;
}

.wxSearchKeyItem{
  flex: none;
  max-width:300rpx;
  white-space:nowrap;
  text-overflow:ellipsis;
  overflow:hidden;
  line-height: 70rpx;
  height: 70rpx;
  background: #fff;
  border-radius: 10rpx;
  color:#212121;
  text-align: center;
  border:1rpx solid #ebebeb;
  font-size: 28rpx;
  margin: 10rpx;
  padding: 0 20rpx;
}
.wxSearchHistoryItem{
  padding: 10rpx 20rpx;
  border-bottom: 1rpx solid #eee;
  display: flex;
  height: 60rpx;
  line-height: 60rpx;
  align-items:center;
}
.wxSearchHistoryItemText{
  flex: 8;
  font-size: 28rpx;
  color: #212121;
  white-space:nowrap;
  text-overflow:ellipsis;
  overflow:hidden;
}
.wxSearchHistoryItemDel{
  display:flex;
  align-items:center;
}
.wxSearchDeleteAllBtn{
  text-align: center;
  color: #999;
  line-height: 60rpx;
  height:60rpx;
  font-size: 28rpx;
  display: block;
  padding: 10rpx 0;
}