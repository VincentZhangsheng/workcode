/* pages/login/login.wxss */
.logo{
    width: 130rpx;
    height: 130rpx;
    display: block;
    margin: 190rpx auto 0;
    overflow: hidden;
    border-radius: 100%
}

.logo image{
    width: 100%;
    height: 100%;
}

.name{
    font-size: 30rpx;
    color: #333;
    display: block;
    text-align: center;
    width: 130rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    margin:10rpx auto;
}
.phone{
    font-size: 24rpx;
    color: #999;
    text-align: center;
    display: block;
    margin-bottom: 120rpx;
}

.btn{
    width: 595rpx;
    height: 78rpx;
    font-size: 30rpx;
    color: #666;
    border:1rpx solid #e0e0e0;
    background: #fff;
    margin-bottom: 40rpx;
    /*border-radius: 5rpx;*/
}
.btn.login{
    background: #ff7919;
    border-color:#ff7919;
    color: #fff;
}