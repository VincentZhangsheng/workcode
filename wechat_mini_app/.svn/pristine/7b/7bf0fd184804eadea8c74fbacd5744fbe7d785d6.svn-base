/* pages/myjuooo/address/index.wxss */
@import "../../component/nullTip/nullTip.wxss";

page {
    background: #fafafa;
}

/*地址列表*/
.addr-item {
    border-bottom: 1px solid #e8e8e8;
    padding: 30rpx 0 30rpx 30rpx;
    background: #fff;
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.addr-item:last-child {
    border-bottom: none;
}

.addr-item .icon-circle {
  font-size:40rpx;
}
.addr-item .icon-wrap {
  padding-right: 20rpx;
}
.addr-item .item-left {
    flex: auto;
}
.addr-item .item-right {
    padding: 10rpx 30rpx 10rpx 40rpx;
    text-align: center;
    border-left: 1px solid #e8e8e8;
}
.addr-item .person {
    font-size: 30rpx;
    color: #212121;
    margin-bottom: 20rpx;
    display: flex;
}
.addr-item .person .name {
    display: inline-block;
    margin-right: 20rpx;
    width: 180rpx;
    white-space:nowrap;
    text-overflow:ellipsis;
    overflow:hidden;
}
.addr-item .addr {
    font-size: 24rpx;
    max-width: 500rpx;
    white-space:nowrap;
    text-overflow:ellipsis;
    overflow:hidden;
    color: #999;
}
.addr-item .default {
    margin-right: 15rpx;
    color: #ff7919;
}
/*地址列表END*/

.footer {
  width: 100%;
  height: 110rpx;
  background: #fff;
  border-top: 1px solid #f0f0f0;
  position: fixed;
  bottom: 0;
  left: 0;
  display:flex;
  align-items:center;
  justify-content:center;
}
.footer .new-btn {
  flex-grow: 1;
  text-align: center;
  margin: 0 20rpx;
  background: #ff7919;
  height: 86rpx;
  line-height: 86rpx;
  color: #fff;
  font-size: 32rpx;
  border-radius: 8rpx;
}

.footer text {
  display: inline-block;
  margin-right: -5rpx;
}



