/**app.wxss**/

page{
  font-family: '黑体';
}

/*左上角标签*/
.icon-flag{
  position:absolute;
  top: 10rpx;
  font-size: 22rpx;
  color: #fff;
  border-left:20rpx solid rgba(255,121,25,0.8);
  border-right:20rpx solid transparent;
  border-bottom:20rpx solid rgba(255,121,25,0.8);
  border-top:20rpx solid rgba(255,121,25,0.8);
  width:50rpx;
  height:0;
  line-height: 0;
}

/*预订标签*/
.icon-destine {
  position:absolute;
  top: 10rpx;
  width:75rpx;
  height: 40rpx;
  background-image: url(data:image/png;base64,);
  background-size: contain;
  background-repeat: no-repeat;
}
/*预售标签*/
.icon-presell {
  position:absolute;
  top: 10rpx;
  width:75rpx;
  height: 40rpx;
  background-image: url(data:image/png;base64,);
  background-size: contain;
  background-repeat: no-repeat;
}

/*主办标签*/
.icon-host{
    position:absolute;
    content: ''; 
    width: 58rpx; 
    height: 70rpx;
    right: 0;
    background-image: url(data:image/png;base64,);
    background-size: contain;
    background-repeat: no-repeat;
}
.icon-host.xcb-host{
    background-image: url(data:image/png;base64,);
    background-size: contain;
    background-repeat: no-repeat;
}

/*方向箭头 默认向右*/
.icon-arrow{
    display: inline-block;
    overflow: visible;
    margin: 0 10rpx;
}
.icon-arrow::after {
    content: '';
    display: block;
    height: 12rpx;
    width: 12rpx;
    border-width: 1px 1px 0 0;
    border-style: solid;
    transform: rotate(45deg);
}
.icon-arrow.up{
  vertical-align: baseline;
}
.icon-arrow.up::after {
  transform: rotate(-45deg);
}
.icon-arrow.down{
  margin-bottom: 5rpx;
}
.icon-arrow.down::after {
  transform: rotate(135deg);
}


/*loading icon*/
.icon-loading{
    display: inline-block;
    width: 30rpx; 
    height: 30rpx;
    /*background: url(data:image/gif;base64,) no-repeat; */
    background: url(data:image/gif;base64,) no-repeat;
    background-size: contain;
}



